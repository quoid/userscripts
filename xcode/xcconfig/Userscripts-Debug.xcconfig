EXT_IDENTIFIER = $(APP_IDENTIFIER).ext.safari
// App Groups and Group Containers
IOS_SHARED_GID = group.$(ORG_IDENTIFIER).userscripts
MAC_SHARED_GID = 00000.$(ORG_IDENTIFIER).userscripts
// App URL Scheme
APP_URL_SCHEME = userscriptsdebug

